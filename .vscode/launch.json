{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Tools/FactorySimulation/OnPremAssets/Station/bin/Debug/net6.0/Station.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Tools/FactorySimulation/OnPremAssets/Station",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "StationType": "assembly",
                "StationURI": "opc.tcp://assembly.munich/",
                "PowerConsumption": "200",
                "CycleTime": "6",
                "STORAGE_CONTAINER_NAME": "ontologies",
                "STORAGE_CONNECTION_STRING": "DefaultEndpointsProtocol=https;AccountName=ontologiesdebug;AccountKey=k/wZ919u9PaS+APje5guWCMd8uykuIVE2EcPwKJ126vsYpMQPcUA9u+i+cs7p51tYTQAnLODtrFj+AStk20Whw==;EndpointSuffix=core.windows.net"      
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "Launch .NET Core in Docker",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "Run Docker Container",
            "netCore": {
                "appProject": "${workspaceFolder}/Tools/FactorySimulation/OnPremAssets/Station/Station.csproj",
                "env": {
                    "StationType": "assembly",
                    "StationURI": "opc.tcp://assembly.munich/",
                    "PowerConsumption": "200",
                    "CycleTime": "6",
                    "STORAGE_CONTAINER_NAME": "ontologies",
                    "STORAGE_CONNECTION_STRING": "DefaultEndpointsProtocol=https;AccountName=ontologiesdebug;AccountKey=k/wZ919u9PaS+APje5guWCMd8uykuIVE2EcPwKJ126vsYpMQPcUA9u+i+cs7p51tYTQAnLODtrFj+AStk20Whw==;EndpointSuffix=core.windows.net"      
                }
            }
        },
        {
            "name": "Docker .NET Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/Tools/FactorySimulation/OnPremAssets/Station/Station.csproj",
                "env": {
                    "StationType": "assembly",
                    "StationURI": "opc.tcp://assembly.munich/",
                    "PowerConsumption": "200",
                    "CycleTime": "6",
                    "STORAGE_CONTAINER_NAME": "ontologies",
                    "STORAGE_CONNECTION_STRING": "DefaultEndpointsProtocol=https;AccountName=ontologiesdebug;AccountKey=k/wZ919u9PaS+APje5guWCMd8uykuIVE2EcPwKJ126vsYpMQPcUA9u+i+cs7p51tYTQAnLODtrFj+AStk20Whw==;EndpointSuffix=core.windows.net"      
                }
            }
        }
    ]
}